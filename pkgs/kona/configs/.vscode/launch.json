{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Jest",
            "type": "node",
            "request": "launch",
            "runtimeArgs": [
                "--inspect-brk",
                "${workspaceRoot}/node_modules/.bin/jest",
                "${relativeFile}",
                "--runInBand",
                "--watch"
            ],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "port": 9229
        }
    ]
}
